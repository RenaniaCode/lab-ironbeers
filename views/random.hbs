<div id="random">
    {{#each randomAPI}}
    <img src=" {{image_url}} " alt="">
    <section>
        <div id="info">
            <h1>{{name}}</h1>
            <h3>{{tagline}}</h3>
            <div>
                <p>{{description}}</p>
            </div>
            <h2 id="food-pairing">Food Pairing</h2>
            <ul class="pairing">
                {{#each food_pairing}}
                <li> {{this}} </li>
                {{/each}}
            </ul>
            <div>
                <p><b>Brewers Tips:</b> "{{brewers_tips}}"</p>
            </div>
        </div>
    </section>
    {{/each}}
</div>

